{
  "name": "payloadkit-blank",
  "version": "0.1.0",
  "description": "A PayloadKit blank template",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate:types": "payload generate:types",
    "generate:importmap": "payload generate:importmap",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "docker:dev": "docker-compose up",
    "docker:dev:build": "docker-compose up --build",
    "docker:dev:detached": "docker-compose up -d",
    "docker:dev:full": "docker-compose --profile full up",
    "docker:stop": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up --build",
    "docker:logs": "docker-compose logs -f app",
    "docker:db": "docker-compose exec postgres psql -U payloadkit -d payloadkit_dev",
    "docker:db-only": "docker-compose -f docker-compose.db-only.yml up",
    "docker:db-only:detached": "docker-compose -f docker-compose.db-only.yml up -d",
    "docker:db-only:stop": "docker-compose -f docker-compose.db-only.yml down",
    "docker:db-only:pgadmin": "docker-compose -f docker-compose.db-only.yml --profile pgadmin up"
  },
  "dependencies": {
    "payload": "^3.56.0",
    "next": "^15.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "@payloadcms/db-postgres": "^3.56.0",
    "@payloadcms/db-mongodb": "^3.56.0",
    "@payloadcms/email-nodemailer": "^3.56.0",
    "@payloadcms/richtext-lexical": "^3.56.0",
    "@payloadcms/next": "^3.56.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "lucide-react": "^0.436.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-slot": "^1.0.2",
    "payload-auditor": "^1.8.0",
    "express-rate-limit": "^7.1.5",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "typescript": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "autoprefixer": "^10.4.0",
    "postcss": "^8.4.0",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.0.0"
  }
}