{
  "name": "payloadkit-blank",
  "version": "0.3.2",
  "description": "A PayloadKit blank template",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "generate:types": "payload generate:types",
    "generate:importmap": "payload generate:importmap",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "docker:dev": "docker-compose up",
    "docker:dev:build": "docker-compose up --build",
    "docker:dev:detached": "docker-compose up -d",
    "docker:dev:full": "docker-compose --profile full up",
    "docker:stop": "docker-compose down",
    "docker:reset": "docker-compose down -v && docker-compose up --build",
    "docker:logs": "docker-compose logs -f app",
    "docker:db": "docker-compose exec postgres psql -U payloadkit -d payloadkit_dev",
    "docker:db-only": "docker-compose -f docker-compose.db-only.yml up",
    "docker:db-only:detached": "docker-compose -f docker-compose.db-only.yml up -d",
    "docker:db-only:stop": "docker-compose -f docker-compose.db-only.yml down",
    "docker:db-only:pgadmin": "docker-compose -f docker-compose.db-only.yml --profile pgadmin up"
  },
  "dependencies": {
    "payload": "3.55.1",
    "next": "15.4.4",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^7.0.5",
    "prism-react-renderer": "^2.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-hook-form": "7.45.4",
    "@payloadcms/db-postgres": "3.55.1",
    "@payloadcms/db-mongodb": "3.55.1",
    "@payloadcms/email-nodemailer": "3.55.1",
    "@payloadcms/richtext-lexical": "3.55.1",
    "@payloadcms/next": "3.55.1",
    "@payloadcms/live-preview-react": "3.55.1",
    "@payloadcms/payload-cloud": "3.55.1",
    "@payloadcms/plugin-form-builder": "3.55.1",
    "@payloadcms/plugin-import-export": "3.55.1",
    "@payloadcms/plugin-nested-docs": "3.55.1",
    "@payloadcms/plugin-redirects": "3.55.1",
    "@payloadcms/plugin-search": "3.55.1",
    "@payloadcms/plugin-seo": "3.55.1",
    "@payloadcms/ui": "3.55.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "color": "^5.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "geist": "^1.4.2",
    "graphql": "^16.11.0",
    "install": "^0.13.0",
    "motion": "^12.23.12",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "lucide-react": "^0.436.0",
    "radix-ui": "^1.4.3",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "payload-auditor": "^1.8.0",
    "express-rate-limit": "^7.1.5",
    "sharp": "^0.34.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@playwright/test": "1.54.1",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/react": "16.3.0",
    "@types/color": "^4.2.0",
    "@types/escape-html": "^1.0.4",
    "@types/node": "22.5.4",
